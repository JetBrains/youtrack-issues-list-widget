{
  "name": "youtrack-issues-list-widget",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src",
    "dist": "./dist"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "create-component": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "build": "webpack -p --devtool sourcemap",
    "test": "karma start",
    "lint": "eslint . --quiet=true && npm run stylelint",
    "stylelint": "stylelint --ignore-path .gitignore **/*.scss",
    "ci-test": "npm run lint && xvfb-maybe karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port 9010",
    "i18n:extract": "angular-gettext-cli --files \"{src/**/*.js,node_modules/hub-dashboard-addons/components/**/*.js,node_modules/@jetbrains/hub-widget-ui/src/**/*.js}\" --dest src/app/translations/default.pot --marker-name=i18n --module-method-plural=plural --module-name=i18n"
  },
  "babel": {
    "presets": "@jetbrains/jetbrains"
  },
  "dependencies": {
    "@jetbrains/babel-preset-jetbrains": "1.0.1",
    "@jetbrains/hub-widget-ui": "^1.0.28",
    "@jetbrains/icons": "1.12.0",
    "@jetbrains/logos": "0.1.6",
    "@jetbrains/ring-ui": "^1.0.35",
    "angular-gettext-cli": "1.2.0",
    "angular-gettext-loader": "1.0.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "css-loader": "0.28.7",
    "fecha": "3.0.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "3.2.0",
    "hub-dashboard-addons": "0.2.2",
    "json-loader": "0.5.7",
    "node-sass": "4.10.0",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.0.9",
    "postcss-modules-values-replace": "1.2.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "redbox-react": "1.3.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "4.28.1",
    "webpack-dev-server": "3.1.14"
  },
  "devDependencies": {
    "eslint-import-resolver-webpack": "^0.11.0",
    "@jetbrains/eslint-config": "^4.0.3",
    "@jetbrains/generator-ring-ui": "0.4.5",
    "@jetbrains/stylelint-config": "1.1.35",
    "babel-eslint": "^10.0.1",
    "chai": "4.0.2",
    "chai-as-promised": "7.0.0",
    "chai-dom": "1.5.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-spies": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "electron": "3.0.10",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "^5.12.1",
    "json-loader": "0.5.7",
    "karma": "3.1.4",
    "karma-chai-plugins": "0.9.0",
    "karma-electron-launcher": "0.2.0",
    "karma-mocha": "1.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.1.0",
    "karma-webpack": "3.0.5",
    "mocha": "5.1.1",
    "react-test-renderer": "16.2.0",
    "sinon": "2.3.5",
    "sinon-chai": "2.11.0",
    "stylelint": "7.11.1",
    "webpack-cli": "3.2.1",
    "xvfb-maybe": "0.2.1",
    "yo": "1.8.5"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
